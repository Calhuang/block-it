{
  "manifest_version": 2,
  "name": "image blocker",
  "version": "0.1",
  "browser_action": {
  	"default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ]
}
